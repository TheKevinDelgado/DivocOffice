<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load">
  <ribbon>
    <tabs>
      <tab idMso="TabHome">
        <group id="DivocGroup" getLabel="OnGetLabel" getImage="OnGetImage">
          <button id="SavePresentation" size="large" getLabel="OnGetLabel" onAction="OnAction" getEnabled="OnGetEnabled" getImage="OnGetImage"/>
          <button id="OpenPresentation" size="large" getLabel="OnGetLabel" onAction="OnAction" getEnabled="OnGetEnabled" getImage="OnGetImage"/>
        </group>
      </tab>
    </tabs>
  </ribbon>

  <backstage>
    <tab idMso="TabRecent">
      <firstColumn>
        <taskFormGroup idMso="GroupOpenPlaces">
          <category idMso="ButtonCategoryRecentDocuments">
            <task id="DivocOpenTask" insertAfterMso="ButtonTaskRecentDocuments" getImage="OnGetImage" getLabel="OnGetLabel">
              <group id="DivocOpenTaskGroup" getLabel="OnGetLabel" getHelperText="OnGetHelperText">
                <primaryItem>
                  <button id="OpenPresentationBackstage" getImage="OnGetImage" getLabel="OnGetLabel" onAction="OnAction"/>
                </primaryItem>
              </group>
            </task>
          </category>
        </taskFormGroup>
      </firstColumn>
    </tab>
  </backstage>  
</customUI>

